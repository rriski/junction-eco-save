{
  "name": "smart-property-management",
  "version": "1.0.0",
  "scripts": {
    "start": "blitz start",
    "studio": "blitz db studio",
    "build": "blitz build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx . && stylelint app",
    "lint:fix": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx . --fix && stylelint app --fix",
    "stylelint": "stylelint app",
    "stylelint:fix": "stylelint app --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "nuke": "rm -rf .blitz && rm -rf node_modules"
  },
  "browserslist": [
    "defaults"
  ],
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix && stylelint --fix"
    ]
  },
  "dependencies": {
    "@prisma/cli": "2.10.2",
    "@prisma/client": "2.10.2",
    "@svgr/webpack": "5.4.0",
    "blitz": "0.24.3",
    "final-form": "4.20.1",
    "next": "10.0.1",
    "next-compose-plugins": "2.2.0",
    "next-transpile-modules": "4.1.0",
    "ol": "6.4.3",
    "react": "0.0.0-experimental-7f28234f8",
    "react-content-loader": "5.1.4",
    "react-dom": "0.0.0-experimental-7f28234f8",
    "react-error-boundary": "2.3.2",
    "react-final-form": "6.5.2",
    "secure-password": "4.0.0",
    "styled-components": "5.2.1",
    "styled-layout": "0.4.0",
    "zod": "1.11.10"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.1",
    "@testing-library/react-hooks": "3.4.2",
    "@types/jest": "26.0.15",
    "@types/ol": "6.4.2",
    "@types/react": "16.9.56",
    "@types/secure-password": "3.1.0",
    "@types/styled-components": "5.1.4",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.12.1",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.1",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "react-test-renderer": "16.14.0",
    "stylelint": "13.7.2",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-order": "4.1.0",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5"
